{
  "name": "StartUC3M Amazon Redirecter",
  "description": "Redirects normal Amazon links to StartUC3M affiliate links",
  "version": "1.0",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "permissions": [
    "webRequest",
    "*://amazon.es/*",
    "*://www.amazon.es/*",
    "webRequestBlocking"
  ],
  "icons" : {
    "16" : "images/start16.png",
    "48" : "images/start48.png",
    "128" : "images/start128.png"
  }
}